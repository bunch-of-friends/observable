{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "declaration": true,
      "outDir": "dist",
      "noImplicitAny": true,
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "target": "es5",
      "lib": [
        "dom",
        "es2015"
      ]
    },
    "exclude": [
      "node_modules",
      "dist",
      "tests"
    ]
  }